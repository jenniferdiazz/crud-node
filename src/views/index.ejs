  <body>
    <div class="container">
      <h1 class="text-center">Formulario</h1>
   
    <form action="/add" method="POST">
        <div class="form-row">
          <div class="form-group col-md-4">
            <label for="inputEmail4">Numero de Serie</label>
            <input type="text" class="form-control" id="inputEmail4" name="nSerie">
          </div>
          <div class="form-group col-md-4">
            <label for="inputPassword4">Patente</label>
            <input type="text" class="form-control" id="inputPassword4" name="patente">
          </div>
          <div class="form-group col-md-4">
            <label for="inputAddress">KM inicial</label>
            <input type="text" class="form-control" id="inputAddress"  name="kmI">
          </div>
        </div>
       
      
        <div class="form-row">
          <div class="form-group col-md-4">
            <label for="inputAddress2">Horometro Inicial</label>
            <input type="text" class="form-control" id="inputAddress2" name="horometroI">
          </div>
          <div class="form-group col-md-4">
            <label for="">Combustible inicial</label>
            <input type="text" class="form-control" id="combustible" name="combustibleI">
          </div>
          <div class="form-group col-md-4">
            <label for="">Nombre de empresa</label>
            <input type="text" class="form-control" id="empresa" name="nombreE">
          </div>
          
        </div>
        <div class="form-row">
          <div class="form-group col-md-4">
            <label for="" name="">Chofer asignado</label>
            <input type="text" class="form-control" id="chofer" name="chofer">
          </div>
          <div class="form-group col-md-4">
            <label for="inputFechaI">Fecha de instalacion</label>
            <input type="text" class="form-control" id="fechaI" name="fechaI">
          </div>
          <div class="form-group col-md-4">
            <label for="inputFechaA">Fecha de activacion</label>
            <input type="text" class="form-control" id="fechaA" name="fechaA">
          </div>
        </div>
        <div class="text-center">
          <button type="submit" class="btn btn-primary">Guardar</button>
        </div>
          
    
      </form>
      <br><br><br>
      <div class="text-center">
        <button type="text" class="btn btn-primary btn-large" onclick="location.href= 'listado'"  id="myButton">Ver listado</button>
      </div>
     </div>





   

    

